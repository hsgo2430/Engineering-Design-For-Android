<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Button
        android:id="@+id/go_to_navigation_activity"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="네비게이션"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="@id/go_to_chat_bot_activity"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <Button
        android:id="@+id/go_to_chat_bot_activity"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="챗봇"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/go_to_navigation_activity"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <Button
        android:id="@+id/go_to_kakao_map_activity"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="카카오지도"
        app:layout_constraintTop_toBottomOf="@id/go_to_navigation_activity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>